<div id="influencer_ranking">
  <table class="new_counselings">
  <tr class="tr_image">
    <th>画像</th>
    <th>画像</th>
    <th>画像</th>
    <th>画像</th>
    <th>画像</th>
  </tr>
  <tr class="tr_title">
    <% @influencers.first(5).each do |influencer| %>
      <td><%= influencer.name %></td>
    <% end %>
  </tr>
  <tr class="tr_date">
    <% @influencers.first(5).each do |influencer| %>
      <td><%= influencer.profile %></td>
    <% end %>
  </tr>
  <tr class="tr_people">
    <% @influencers.first(5).each do |influencer| %>
      <td><%= influencer.genre %></td>
    <% end %>
  </tr>
  <tr class="tr_button">
    <% @influencers.first(5).each do |influencer| %>
      <td><%= link_to "詳細", influencer_path(influencer) %></td>
    <% end %>
  </tr>
</table>
</div>
