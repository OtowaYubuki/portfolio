<table class="new_counselings">
  <tr class="tr_image">
    <% @reservation_counselings.each do |counseling| %>
      <%= image_tag '25307560_s.jpg', size:'100x100' %>
    <% end %>
  </tr>
  <tr class="tr_title">
    <% @reservation_counselings.each do |counseling| %>
      <td><%= counseling.name %></td>
    <% end %>
  </tr>
  <tr class="tr_date">
    <% @reservation_counselings.each do |counseling| %>
      <td><%= l counseling.day, format: :long %></td>
    <% end %>
  </tr>
  <tr class="tr_date">
    <% @reservation_counselings.each do |counseling| %>
      <td><%= l counseling.starttime, format: :short %></td>
    <% end %>
  </tr>
  <tr class="tr_button">
    <% @reservation_counselings.each do |counseling| %>
      <td><%= link_to "詳細", counseling_path(counseling) %></td>
    <% end %>
  </tr>
</table>
