# 新着カウンセリング
<table class="new_counselings">
  <tr class="tr_image">
    <th>画像</th>
    <th>画像</th>
    <th>画像</th>
    <th>画像</th>
    <th>画像</th>
  </tr>
  <tr class="tr_title">
    <% @counselings.first(5).each do |counseling| %>
      <td><%= counseling.name %></td>
    <% end %>
  </tr>
  <tr class="tr_date">
    <% @counselings.first(5).each do |counseling| %>
      <td><%= counseling.date %></td>
    <% end %>
  </tr>
  <tr class="tr_people">
    <% @counselings.first(5).each do |counseling| %>
      <td><%= counseling.people %></td>
    <% end %>
  </tr>
  <tr class="tr_button">
    <% @counselings.first(5).each do |counseling| %>
      <td><%= link_to "予約する", new_counseling_reservations_path(counseling) %></td>
    <% end %>
  </tr>
</table>

